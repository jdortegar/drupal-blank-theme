{% include directory ~ '/partials/header.html.twig' %}

<!-- CAROUSEL  -->
<div class="carousel-container">
  <div id="carouselHome" class="carousel slide">
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img src="./themes/custom/gftheme/images/gf-back.png" alt="Global Foresight" class="img-fluid" />
        <div class="carousel-content">
          <div class="col-sm-11 col-xl-4 offset-sm-1">
            <div class="row align-items-center">
              <div class="col-sm-4 col-xl-12 logo-section-1">
                <img src="./themes/custom/gftheme/images/gf-logo.png" alt="Global Foresight" class="img-fluid" />
              </div>
              <div class="col-sm-7 col-xl-12">
                <h5 class="white-color">Somos una organización que identifica y genera nuevas oportunidades en contextos
                  desafiantes. Así, contribuimos a la promoción de empresas y generación de nuevas compañías, con
                  modelos
                  de
                  negocios centrados en dinámicas de las economías emergentes.</h5>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

